<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="818dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="0dp"
        android:backgroundTint="#1B30A3"
        app:srcCompat="@drawable/ww" />

    <Button
        android:id="@+id/button"
        android:layout_width="412dp"
        android:layout_height="88dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="-1dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="209dp"
        android:backgroundTint="#11756C"
        android:onClick="CURSOS"
        android:text="@string/CURSOS"
        android:textSize="18sp"
        app:icon="@android:drawable/ic_menu_save" />

    <Button
        android:id="@+id/QUIEN"
        android:layout_width="165dp"
        android:layout_height="105dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="91dp"
        android:backgroundTint="#636C0C"
        android:onClick="QUIEN"
        android:text="@string/SOMOS"
        android:textSize="18sp"
        app:icon="@android:drawable/btn_star_big_on" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="65dp"
        android:layout_height="74dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="296dp"
        android:layout_marginTop="694dp"
        android:layout_marginBottom="51dp"
        app:srcCompat="@drawable/_80b57fcd9996e24bc43c543" />

    <Button
        android:id="@+id/btn_link"
        android:layout_width="wrap_content"
        android:layout_height="99dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="50dp"
        android:backgroundTint="#9F2F0C"
        android:onClick="CURSOS"
        android:text="CONTACTOS"
        app:icon="@android:drawable/sym_call_incoming" />

</RelativeLayout>